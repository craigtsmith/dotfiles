# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[hub]
        protocol = https
[alias]
        co = checkout
        count   = !git shortlog -sn
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
	excludesfile = ~/.dotfiles/lib/git/gitignore
        editor = cursor
	autocrlf = input
[apply]
        whitespace = nowarn
[help]
        autocorrect = 1
[push]
        default = simple